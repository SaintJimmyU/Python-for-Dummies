"""Declaracion ejemplo 1"""
print("Resultado Declaracion ejemplo 1")
print((lambda num: num*3)) 

"""Declaracion ejemplo 2"""
print("Resultado Declaracion ejemplo 2")
print((lambda num: num*3)(2,)) 

"""Declaracion ejemplo 3"""
print("Resultado Declaracion ejemplo 3")
multiplicacion = lambda num: num*2

print(multiplicacion(2))
print(multiplicacion(9))
